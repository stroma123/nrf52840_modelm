    ansi84_matrix_transform: ansi84_matrix_transform {
        compatible = "zmk,matrix-transform";
        rows = <8>;
        columns = <16>;
        
        //  -------------------------------------------------------------------------------------------------------------------------
        //  | ESC   |       | F1    | F2    | F3    | F4    |   | F5    | F6    | F7    | F8    |   | F9    | F10   | F11   | F12   |       |PRT SCR| SLCK  |PAU_BRK|
        //  |  `    |  1    |  2    |  3    |  4    |  5    |  6    |  7    |  8    |  9    |  0    |  -    |  =    |   BKSP        |       |INSERT | HOME  |PAGE_UP|
        //  |    TAB    |  Q    |  W    |  E    |  R    |  T    |  Y    |  U    |  I    |  O    |  P    |   [   |   ]   |   "|\"    |       |DELETE |  END  |PAGE_DW|
        //  | CAPS        |   A   |   S   |   D   |   F   |   G   |   H   |   J   |   K   |   L   |   ;   |   '   |      ENTER      |                                
        //  |   LSHIFT        |   Z   |   X   |   C   |   V   |   B   |   N   |   M   |   ,   |   .   |   /   |        RSHIFT       |               |  UP   |        
        //  |   CTRL    |       |    ALT    |                        SPACE                          |   ALTGR   |       |   CTRL    |       | LEFT  | DOWN  | RIGHT |
        //  -------------------------------------------------------------------------------------------------------------------------
        map = <
        //  -------------------------------------------------------------------------------------------------------------------------
        //  | ESC   |       | F1    | F2    | F3    | F4    |   | F5    | F6    | F7    | F8    |   | F9    | F10   | F11   | F12   |       |PRT SCR| SLCK  |PAU_BRK|
            RC(7,2)         RC(5,3) RC(5,4) RC(6,4) RC(7,4)     RC(7,6) RC(7,8) RC(6,9) RC(5,9)     RC(5,6) RC(4,6) RC(4,11) RC(4,12)      RC(4,15) RC(3,15) RC(1,14)

        //  |  `    |  1    |  2    |  3    |  4    |  5    |  6    |  7    |  8    |  9    |  0    |  -    |  =    |   BKSP        |       |INSERT | HOME  |PAGE_UP|
            RC(5,2) RC(4,2) RC(4,3) RC(4,4) RC(4,5) RC(5,5) RC(5,7) RC(4,7) RC(4,8) RC(4,9) RC(4,10) RC(5,10) RC(5,8)   RC(6,6)            RC(5,12) RC(5,14) RC(5,13)

        //  |    TAB    |  Q    |  W    |  E    |  R    |  T    |  Y    |  U    |  I    |  O    |  P    |   [   |   ]   |   "|\"    |       |DELETE |  END  |PAGE_DW|
               RC(6,2)  RC(3,2) RC(3,3) RC(3,4) RC(3,5) RC(6,5) RC(6,7) RC(3,7) RC(3,8) RC(3,9) RC(3,10) RC(6,10) RC(6,8) RC(2,6)          RC(5,11) RC(4,14) RC(4,13)

        //  | CAPS        |   A   |   S   |   D   |   F   |   G   |   H   |   J   |   K   |   L   |   ;   |   '   |      ENTER      |                                
            RC(6,3)       RC(2,2) RC(2,3) RC(2,4) RC(2,5) RC(7,5) RC(7,7) RC(2,7) RC(2,8) RC(2,9) RC(2,10) RC(7,10)     RC(1,6)                                      

        //  |   LSHIFT        |   Z   |   X   |   C   |   V   |   B   |   N   |   M   |   ,   |   .   |   /   |        RSHIFT       |               |  UP   |        
            RC(6,1)           RC(1,2) RC(1,3) RC(1,4) RC(1,5) RC(0,5) RC(0,7) RC(1,7) RC(1,8) RC(1,9) RC(0,10)          RC(1,1)                     RC(7,14)         

        //  |   CTRL    |       |    ALT    |                        SPACE                          |   ALTGR   |       |   CTRL    |       | LEFT  | DOWN  | RIGHT |
            RC(5,0)                RC(7,15)                         RC(0,6)                            RC(0,15)            RC(1,0)         RC(0,14) RC(0,11) RC(0,12)
        //  -------------------------------------------------------------------------------------------------------------------------
        >;
    };
    
};